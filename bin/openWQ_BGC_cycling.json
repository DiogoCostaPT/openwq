{
    
        "MODULE_NAME": "OPENWQ_NATIVE_BGC",
    
        "CHEMICAL_SPECIES": {
            "LIST": {
                "1": "NO3",
                "2": "NH4",
                "3": "N2",
                "4": "DON",
                "5": "labile_orgN",
                "6": "refractory_orgN",
                "7": "SRP",
                "8": "DOP",
                "9": "partP",
                "10": "labile_orgP",
                "11": "refractory_orgP"
            },
            "BGC_GENERAL_MOBILE_SPECIES": [1,2,4,7,8,9]
        },
        "CYCLING_FRAMEWORKS": {
            "N_inorg": {
                "LIST_TRANSFORMATIONS":{
                    "1": "nitrification",
                    "2": "denitrification"
                },
                "1":{
                    "CONSUMED": "NH4",
                    "PRODUCED": "NO3",
